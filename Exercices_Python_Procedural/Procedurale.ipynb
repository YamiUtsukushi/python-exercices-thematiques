    "\"\"\"\n",
    "沐ｹ Exercice 7 : Carnet de notes automatisﾃｩ\n",
    "\"\"\"\n",
    "\n",
    "nb_etudiant = int(input(\"Entrez le nombre d'ﾃｩlﾃｨves : \"))\n",
    "eleves = {}\n",
    "\n",
    "for i in range(nb_etudiant):\n",
    "    nom = input(\"Entrez le nom de l'ﾃｩlﾃｨve : \")\n",
    "    note = float(input(\"Entrez la note de l'ﾃｩlﾃｨve : \"))\n",
    "    eleves[nom] = note\n",
    "\n",
    "somme = 0\n",
    "for note in eleves.values():\n",
    "    somme += note\n",
    "\n",
    "moyenne = somme / nb_etudiant\n",
    "print(\"Moyenne gﾃｩnﾃｩrale :\", moyenne)\n",
    "\n",
    "for eleve_nom, eleve_note in eleves.items():\n",
    "    if eleve_note > 10:\n",
    "        print(f\"{eleve_nom} a eu {eleve_note}/20\")\n"
   ]
